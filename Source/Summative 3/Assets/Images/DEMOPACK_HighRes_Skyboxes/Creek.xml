<demoniak3d>



<scene name="Cubemap_Viewer" display_fps="TRUE" show_ref_grid="TRUE" 
       vsync="TRUE"  > 

	<window_size width="1280" height="1024" />
	<background_color r="0.0" g="0.0" b="0.0" />
	
</scene>




<camera name="Main_Camera" fov="70.0" active="TRUE" navigation_mode="EXAMINE" >
	<position x="0.0" y="100.0" z="200.0" />
	<orientation pitch="20.0" yaw="90.0" />
</camera>






<skybox name="sky" >
	<xpos_face filename="data/Creek/posx.jpg" />
	<xneg_face filename="data/Creek/negx.jpg" />
	<ypos_face filename="data/Creek/posy.jpg" />
	<yneg_face filename="data/Creek/negy.jpg" uv_map_op="SWAP_V" />
	<zpos_face filename="data/Creek/posz.jpg" />
	<zneg_face filename="data/Creek/negz.jpg" uv_map_op="SWAP_U"/>
</skybox>





</demoniak3d>

